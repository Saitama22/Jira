@model IEnumerable<Project>;
@{
    ViewBag.Title = "FindJob - просмотр резюме";
    Layout = "_ProjectLayout";
}
<body>
    @foreach (var project in Model)
    {
        <a asp-action="Project" asp-route-projectId="@project.Id">@project.Name</a>
    }
</body>


